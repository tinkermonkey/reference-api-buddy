{
  "server": {
    "host": "127.0.0.1",
    "port": 8080
  },
  "security": {
    "require_secure_key": true
  },
  "cache": {
    "database_path": "api_buddy_cache.db",
    "default_ttl_seconds": 86400
  },
  "throttling": {
    "default_requests_per_hour": 1000
  },
  "domain_mappings": {
    "example": {
      "upstream": "https://api.example.com"
    }
  },
  "logging": {
    "level": "INFO"
  }
}
